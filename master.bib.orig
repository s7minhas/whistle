@article{colaresi:2004,
	Abstract = {Varied research traditions suggest that dovish leaders
		  will be thrown out of office under harsh external
		  circumstances. Below, I elaborate a model of rivalry
		  maintenance that draws on and refines the insight from
		  studies of leadership tenure and foreign policy.
		  Specifically, I expect a leader who offers unreciprocated
		  cooperation to a rival (a dove) to be more likely to be
		  deselected from power than a leader that takes a harder
		  line vis-\{\`{a}\}-vis the rival (a hawk). I test
		  expectation using event history techniques and data
		  spanning the 1950-1990 time period and find strong evidence
		  that dovish leaders pay an electoral price within a rivalry
		  context. The findings suggest an internationally contingent
		  domestic incentive to maintain rivalry and conflict over
		  time.},
	Author = {Colaresi, Michael},
	Issn = {00925853},
	Journal = {American Journal of Political Science},
	Number = {3},
	Pages = {pp. 555--570},
	Publisher = {Midwest Political Science Association},
	Title = {{When Doves Cry: International Rivalry, Unreciprocated Cooperation, and Leadership Turnover}},
	Volume = {48},
	Year = {2004}}

