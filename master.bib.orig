	Language = {English},
	Number = {1},
	Pages = {pp. 153-174},
	Publisher = {Canadian Political Science Association and the Soci{\'e}t{\'e} qu{\'e}b{\'e}coise de science politique},
	Title = {Counting the Seven Weeks War: Dyads, Disputes and Balances of Power},
