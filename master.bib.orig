@article{greif:laitin:2004,
	Abstract = {This paper asks (a) why and how institutions change, (b)
		  how an institution persists in a changing environment, and
		  (c) how processes that it unleashes lead to its own demise.
		  The paper shows that the game-theoretic notion of
		  self-enforcing equilibrium and the historical
		  institutionalist focus on process are both inadequate to
		  answer these questions. Building on a game-theoretic
		  foundation, but responding to the critique of it by
		  historical institutionalists, the paper introduces the
		  concepts of quasi-parameters and self reinforcement. With
		  these concepts, and building on repeated game theory, a
		  dynamic approach to institutions is offered, one that can
		  account for endogenous change (and stability) of
		  institutions. Contextual accounts of formal governing
		  institutions in early modern Europe and the informal
		  institution of cleavage structure in the contemporary world
		  provide illustrations of the approach.},
	Author = {Greif, Avner and Laitin, David D.},
	Copyright = {Copyright {\copyright} 2004 American Political Science Association},
	Issn = {00030554},
	Journal = {The American Political Science Review},
	Jstor_Articletype = {primary_article},
	Jstor_Formatteddate = {Nov., 2004},
	Number = {4},
	Pages = {633--652},
	Publisher = {American Political Science Association},
	Title = {{A Theory of Endogenous Institutional Change}},
	Volume = {98},
	Year = {2004}}

