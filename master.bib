	Jstor_Articletype = {primary_article},
	Jstor_Formatteddate = {Spring, 1992},
	Number = {2},
	Pages = {391--425},
	Publisher = {The MIT Press},
	Title = {Anarchy is what States Make of it: The Social Construction of Power Politics},
	Volume = {46},
	Year = {1992}}

