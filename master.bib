@article{lorrain:white:1971,
	Abstract = {The aim of this paper is to understand the interrelations among relations
	within concrete social groups. Social structure is sought, not ideal
	types, although the latter are relevant to interrelations among relations.
	From a detailed social network, patterns of global relations can
	be extracted, within which classes of equivalently positioned individuals
	are delineated. The global patterns are derived algebraically through
	a 'functorial' mapping of the original pattern. Such a mapping (essentially
	a generalized homomorphism) allows systematically for concatenation
	of effects through the network. The notion of functorial mapping
	is of central importance in the 'theory of categories,' a branch
	of modem algebra with numerous applications to algebra, topology,
	logic. The paper contains analyses of two social networks, exemplifying
	this approach. [ABSTRACT FROM AUTHOR]},
	Author = {Lorrain, Fran{{\c c}}ois and White, Harrison C.},
