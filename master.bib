	Language = {English},
	Number = {4},
	Pages = {pp. 579-589},
	Publisher = {Blackwell Publishing on behalf of The International Studies Association},
	Title = {The Dearth of Jointly Dyadic Democratic Interventions},
	Volume = {46},
	Year = {2002}}

