	Title = {{Asymmetrical Dyads and Foreign Policy: Canada-U.S. Relations 1963-1972}},
	Volume = {26},
	Year = {1982}}

@article{dolan:tomlin:1984,
	Abstract = {The results of a previous test of a theory of foreign policies of
	subordinate states in asymmetrical dyads conducted on Canada-United
	States relations in the 1963-72 period are used as a foundation for
	a modification of the original theory and a revised empirical test.
	In the reformulated theory, three conditions--the state of the economy
	of the superordinate and the subordinate nations, the extent of concentration
	in the latter's linkages with the superordinate power, and a set
	of environmental conditions--are used to explain the foreign policy
	actions of the subordinate state. The findings indicate considerable
	empirical support for the reformulated theory.},
