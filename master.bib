@article{greif:laitin:2004,
	Abstract = {This paper asks (a) why and how institutions change, (b) how an institution
	persists in a changing environment, and (c) how processes that it
	unleashes lead to its own demise. The paper shows that the game-theoretic
	notion of self-enforcing equilibrium and the historical institutionalist
	focus on process are both inadequate to answer these questions. Building
	on a game-theoretic foundation, but responding to the critique of
	it by historical institutionalists, the paper introduces the concepts
	of quasi-parameters and self reinforcement. With these concepts,
	and building on repeated game theory, a dynamic approach to institutions
	is offered, one that can account for endogenous change (and stability)
	of institutions. Contextual accounts of formal governing institutions
	in early modern Europe and the informal institution of cleavage structure
	in the contemporary world provide illustrations of the approach.},
	Author = {Greif, Avner and Laitin, David D.},
	Copyright = {Copyright {\copyright} 2004 American Political Science Association},
	Date-Added = {2013-05-31 21:52:09 +0000},
	Date-Modified = {2013-05-31 21:52:10 +0000},
