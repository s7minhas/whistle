	Jstor_Issuetitle = {International Organization at Fifty: Exploration and Contestation in the Study of World Politics},
	Number = {4},
	Pages = {887--917},
	Publisher = {The MIT Press},
	Title = {International Norm Dynamics and Political Change},
	Volume = {52},
	Year = {1998}}

