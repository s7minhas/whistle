	Journal = {Political Psychology},
	Jstor_Articletype = {primary_article},
	Jstor_Formatteddate = {Jun., 1992},
	Jstor_Issuetitle = {Special Issue: Prospect Theory and Political Psychology},
	Number = {2},
	Pages = {283--310},
	Publisher = {International Society of Political Psychology},
	Title = {Prospect Theory and International Relations: Theoretical Applications and Analytical Problems},
	Volume = {13},
	Year = {1992}}

