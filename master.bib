	Number = {4},
	Owner = {Audrey},
	Pages = {661-678},
	Publisher = {American Sociological Association},
	Timestamp = {2009.07.22},
	Title = {Neighborhood Social Processes, Physical Conditions, and Disaster-Related Mortality: The Case of the 1995 Chicago Heat Wave},
	Volume = {71},
	Year = {2006}}

