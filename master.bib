	Pages = {569-593},
	Title = {{Shocks to the System: Great Power Rivalry and the Leadership Long Cycle}},
	Volume = {45},
	Year = {2001},
	Bdsk-Url-1 = {http://dx.doi.org/10.1177/0022002701045005002}}

