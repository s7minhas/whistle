	Language = {English},
	Number = {3},
	Pages = {pp. 325-346},
	Publisher = {Blackwell Publishing on behalf of The International Studies Association},
	Title = {The Joint Democracy-Dyadic Conflict Nexus: A Simultaneous Equations Model},
	Volume = {47},
